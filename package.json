{
	"name": "edashboard",
	"version": "1.0.4",
	"description": "A simple dashboard application ",
	"main": "src/main.js",
	"scripts": {
		"start": "nodemon -e js,html --watch ./pages --watch index.js --exec electron src/main.js",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "com.witchcraft.edash",
		"files": [
			"**/*",
			"build/icon.png"
		],
		"target": "NSIS"
	},
	"win": {
		"publish": [
			{
				"provider": "generic",
				"url": "https://10.140.8.134/edash/dist/download"
			}
		]
	},
	"author": "Tanbin Hassan Bappi",
	"license": "Apache-2.0",
	"devDependencies": {
		"electron": "^15.1.2",
		"nodemon": "^2.0.14",
		"electron-builder": "^22.13.1"
	},
	"dependencies": {
		"axios": "^0.23.0",
		"eiows": "^3.6.1",
		"electron-get-location": "^1.0.1",
		"socket.io-client": "^4.3.2"
	}
}
