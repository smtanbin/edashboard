{
	"name": "edashboard",
	"version": "1.0.6",
	"description": "A simple dashboard application",
	"main": "src/main.js",
	"scripts": {
		"start": "nodemon -e js,html --watch /src --watch index.js --exec electron src/main.js",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"test": "echo \"Error: no test specified\" && exit 1",
		"deploy": "electron-builder build --win --publish always"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/smtanbin/edashboard"
	},
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo"
	],
	"build": {
		"appId": "com.standardbankltd.edash",
		"files": [
			"**/*",
			"build/icon.png"
		],
		"target": "NSIS"
	},
	"win": {
		"publish": [
			{
				"provider": "generic",
				"url": "http://10.140.8.127/update/"
			}
		]
	},
	"author": "Tanbin Hassan Bappi",
	"emain": "mds.tanbin@gmail.com",
	"license": "Apache-2.0",
	"devDependencies": {
		"electron": "^15.1.2",
		"nodemon": "^2.0.14",
		"electron-builder": "^22.13.1"
	},
	"dependencies": {
		"axios": "^0.23.0",
		"electron-get-location": "^1.0.1",
		"electron-updater": "^4.3.9",
		"geoip-lite": "^1.4.2",
		"public-ip": "^4.0.4",
		"socket.io-client": "^4.3.2"
	}
}
